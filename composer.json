{
  "name": "paloma/shop-client",
  "description": "Paloma Shop client library",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": ">=7.4",
    "monolog/monolog": ">=3.3",
    "psr/cache": ">=1.0",
    "symfony/http-foundation": ">=6.2",
    "symfony/validator": "^6.2",
    "symfony/yaml": "^6.2",
    "symfony/intl": "^6.2",
    "egulias/email-validator": "^4.0",
    "symfony/serializer": "^6.2",
    "symfony/cache": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Astina",
      "email": "admin@astina.ch"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-0": {
      "Paloma\\Shop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Paloma\\Shop\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  }
}
